<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repository.kasik" name="Kasik's Repository" version="1.0" provider-name="Kasik">
	<extension point="xbmc.addon.repository" name="Kasik's Addon Repository">
		<info compressed="false">https://github.com/Kasik/Kasiks-Repo/raw/master/addons.xml</info>
		<checksum>https://github.com/Kasik/Kasiks-Repo/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/Kasik/Kasiks-Repo/raw/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install all of Kasik's Addons</summary>
		<description>Download and install addons from Kasik.......For Official Support Visit XBMCHUB.COM</description>
		<platform>all</platform>
	</extension>
</addon>


<addon id="plugin.video.tvrule"
       name="TVRule"
       version="0.0.2"
       provider-name="Kasik">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
		<import addon="script.module.urlresolver"  version="0.0.1"/>
        <import addon="script.module.t0mm0.common" version="1.1.0"/>
		<import addon="script.module.metahandler"  version="1.0.2"/>
		<import addon="script.module.universal" version="0.0.1"/>
		<import addon="script.module.beautifulsoup"/>
		<import addon="script.module.playbackengine" version="1.0.0" />
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
    <summary lang="en">Watch Latest TV Shows Online Free</summary>
    <description lang="en">Watch Latest TV Shows Online Free</description>
    <platform>all</platform>
  </extension>
</addon>

</addons>
